<application-mapping xmlns="http://qafe.com/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://qafe.com/schema http://www.qafe.com/schema/2.2/application-mapping.xsd">
   <presentation-tier>
		<view>
			<window id="youtube" displayname="QAFE Introduction" width="510" height="430" icon="images/youtube.png" icon-style="color:white">
				<rootpanel id="myrootpanel">
						<verticallayout>
							<html><text><![CDATA[ <object width="510" height="430"><param name="movie" value="http://www.youtube.com/v/MyiqzrDQQs8&hl=en_GB&fs=1&"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="http://www.youtube.com/v/MyiqzrDQQs8&hl=en_GB&fs=1&" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="510" height="430"></embed></object> ]]></text></html>	
						</verticallayout>
				</rootpanel>		

			</window>
			<window id="youtubechannel" displayname="QAFE Youtube Channel" width="920" height="690" icon="images/youtube.png" in-dock="true">
				<rootpanel id="myrootpanel">
						<verticallayout>
							<frame height="600" width="1024" src="http://www.youtube.com/qafechannel" id="gtalkframe" />
						</verticallayout>
				</rootpanel>		
			</window>

		</view>
   </presentation-tier>

</application-mapping>
